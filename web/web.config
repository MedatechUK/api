<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appSettings>
    <add key="oDataHost" value="http://erpdemo.emerge-it.co.uk:8080" />
  </appSettings>
  <connectionStrings>
    <add connectionString="Server=walrus\PRI;Trusted_Connection=Yes;" name="priority" />
  </connectionStrings>
  <system.webServer>
    <defaultDocument>
      <files>
        <add value="default.ashx" />
      </files>
    </defaultDocument>
    <rewrite>
      <rules>
        <rule name="API Rewrite">
          <match url="^([0-9a-z]+)/([0-9a-z]+).ashx" />
          <action type="Rewrite" url="default.ashx?environment={R:1}" />
        </rule>
      </rules>
    </rewrite>

  </system.webServer>

</configuration>